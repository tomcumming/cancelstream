{
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "strictFunctionTypes": true,
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["."]
}
